<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="gridPositionStyle">
        <item name="android:background">@color/gridPositionColor</item>
        <item name="android:layout_margin">@dimen/gridPositionMargin</item>

    </style>

    <style name="AppAlertTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorPrimary">@color/colorDialog</item>
        <item name="colorAccent">@color/colorDialog</item>
    </style>

</resources>
